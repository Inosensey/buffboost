{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://buffboost-backend-yliv.onrender.com"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "ignoreCommand": "bash -c '! (git rev-parse HEAD^ >/dev/null 2>&1) || git diff --quiet HEAD^ HEAD -- packages/frontend/'"
}
